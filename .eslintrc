{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "extends": ["eslint:recommended"],
    "env": {
        "node": true,
        "es6": true
    },
    "rules": {
        "semi": [2, "never"],
        "comma-dangle": [2, "always"],
        "quotes": [1, "single"],
        "no-console": 0,
        "no-unused-vars": 1

    }
}
